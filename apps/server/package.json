{
    "name": "@gkr/server",
    "version": "0.0.1",
    "description": "gkrjs platform server",
    "author": "南宫漠",
    "private": true,
    "app": true,
    "license": "MIT",
    "scripts": {
        "clean": "rimraf dist",
        "tsnode": "ts-node --files -T -r tsconfig-paths/register",
        "script": "yarn tsnode scripts/bin.ts",
        "cli": "cross-env TS_NODE_PROJECT=tsconfig.start.json yarn tsnode src/main.ts",
        "dev": "cross-env NODE_ENV=development yarn script start --watch",
        "build": "cross-env NODE_ENV=production yarn script build",
        "prod": "cross-env NODE_ENV=production RUN_DIST=dist node ./dist/server.js",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
        "lint": "eslint \"{src,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "webpack": "nest build --webpack --webpackPath webpack-hmr.config.js"
    },
    "dependencies": {
        "@gkr/content": "^0.0.1",
        "@gkr/core": "^0.0.1",
        "@gkr/database": "^0.0.1",
        "@gkr/queue": "^0.0.1",
        "@gkr/redis": "^0.0.1",
        "@gkr/restful": "^0.0.1",
        "@gkr/sms": "^0.0.1",
        "@gkr/user": "^0.0.1",
        "@nestjs/common": "^7.6.8",
        "@nestjs/core": "^7.6.8",
        "@nestjs/jwt": "^7.2.0",
        "@nestjs/passport": "^7.1.5",
        "@nestjs/platform-fastify": "^7.6.8",
        "@nestjs/swagger": "^4.7.12",
        "@nestjs/typeorm": "^7.1.5",
        "@types/passport-local": "^1.0.33",
        "class-transformer": "^0.3.2",
        "class-validator": "^0.13.1",
        "faker": "^5.2.0",
        "fastify-swagger": "^4.0.1",
        "fs-extra": "^9.1.0",
        "lodash": "^4.17.20",
        "mysql2": "^2.2.5",
        "nestjs-typeorm-paginate": "^2.3.1",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.6.3",
        "typeorm": "^0.2.30"
    },
    "devDependencies": {
        "@gkr/fabric": "^0.0.1",
        "@nestjs/cli": "^7.5.4",
        "@nestjs/schematics": "^7.2.7",
        "@nestjs/testing": "^7.6.8",
        "@types/express": "^4.17.11",
        "@types/faker": "^5.1.6",
        "@types/fs-extra": "^9.0.6",
        "@types/jest": "26.0.20",
        "@types/lodash": "^4.14.168",
        "@types/node": "^14.14.22",
        "@types/nodemon": "^1.19.0",
        "@types/passport-jwt": "^3.0.3",
        "@types/start-server-webpack-plugin": "^2.2.0",
        "@types/supertest": "^2.0.10",
        "@types/yargs": "^16.0.0",
        "chalk": "^4.1.0",
        "chokidar": "^3.5.1",
        "clean-webpack-plugin": "^3.0.0",
        "cross-env": "^7.0.3",
        "deepmerge": "^4.2.2",
        "eslint": "7.19.0",
        "find-up": "^5.0.0",
        "fork-ts-checker-webpack-plugin": "^6.1.0",
        "glob": "^7.1.6",
        "jest": "26.6.3",
        "lodash": "^4.17.20",
        "nodemon": "^2.0.7",
        "nodemon-webpack-plugin": "^4.4.4",
        "ora": "^5.3.0",
        "prettier": "^2.2.1",
        "prettier-plugin-organize-imports": "^1.1.1",
        "rimraf": "^3.0.2",
        "run-node-webpack-plugin": "^1.3.0",
        "run-script-webpack-plugin": "^0.0.5",
        "shelljs": "^0.8.4",
        "start-server-nestjs-webpack-plugin": "^2.2.5",
        "start-server-webpack-plugin": "^2.2.5",
        "supertest": "^6.1.3",
        "ts-jest": "26.5.0",
        "ts-loader": "^8.0.14",
        "ts-node": "9.1.1",
        "ts-node-dev": "^1.1.1",
        "tsconfig-paths": "^3.9.0",
        "tsconfig-paths-webpack-plugin": "^3.3.0",
        "typescript": "^4.1.3",
        "webpack": "^5.19.0",
        "webpack-chain": "^6.5.1",
        "webpack-format-messages": "^2.0.6",
        "webpack-merge": "^5.7.3",
        "webpack-node-externals": "^2.5.2",
        "yargs": "^16.2.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": ".",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "./coverage",
        "testEnvironment": "node"
    }
}
